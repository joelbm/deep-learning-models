    <script type="text/javascript">
        tjq(document).ready(function() {
            tjq("#price-range").slider({
                range: true,
                min: 0,
                max: 2000,
                values: [ 10, 1750 ],
                slide: function( event, ui ) {
                    tjq(".min-price-label").html( "€" + ui.values[ 0 ]);
                    tjq(".max-price-label").html( "€" + ui.values[ 1 ]);
                    tjq(".min-price-value").val( ui.values[ 0 ]);
                    tjq(".max-price-value").val( ui.values[ 1 ]);
                                        
                },
                stop: function( event, ui ) {
                  location.href = '/zoeken/?min-price='+ui.values[0]+'&max-price='+ui.values[1];
                }
            });
            tjq(".min-price-label").html( "€" + tjq("#price-range").slider( "values", 0 ));
            tjq(".max-price-label").html( "€" + tjq("#price-range").slider( "values", 1 ));
            
            tjq(".min-price-value").val( tjq("#price-range").slider( "values", 0 ));
            tjq(".max-price-value").val( tjq("#price-range").slider( "values", 1 ));
            
            tjq("#rating").slider({
                range: "min",
                value: 70,
                min: 0,
                max: 100,
                slide: function( event, ui ) {
                    tjq(".min-reviews").val( ui.value);
                },
                stop: function( event, ui ) {
                  location.href = '/zoeken/'+'?reviews='+ui.value;
                }
            });
            
            tjq("#q-search button").click(function(){
            	location.href = '/zoeken/?q='+tjq("#q-search input").val();
            });
            
            tjq("#q-search input").keyup(function(e){
					   	if(e.keyCode == 13){
					    	location.href = '/zoeken/?q='+tjq("#q-search input").val();
					    }
						});
        });
    </script>
